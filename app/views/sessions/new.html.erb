<div class="logsignform">
  <div class="logo">
    <img src="http://i.imgur.com/spbIxDB.png">
  </div>
  <form action="<%= session_url %>" method="post" class="signform">
    <%= auth_token_input %>

    <label class="loginform">
      Username
      <input type="text" name="user[username]" value="">
    </label>

    <label class="loginform">
      Password
      <input type="password" name="user[password]" value="">
    </label>

    <input type="submit" value="Sign In" class="signformbtn">
  </form>
</div>
